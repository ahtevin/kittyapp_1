<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kittycam">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo1"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar  ">
        <activity android:name=".forgotpassword_activity"></activity>
        <activity android:name=".recordedvideo_activity" />
        <activity android:name=".picture_activity" />
        <activity android:name=".notification_activity" />
        <activity android:name=".record_activity" />
        <activity android:name=".Profile_activity" />
        <activity android:name=".entry_activity" />
        <activity android:name=".videoview_activity" />
        <activity android:name=".signup_activity" />
        <activity android:name=".login_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>